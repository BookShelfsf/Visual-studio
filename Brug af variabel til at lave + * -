namespace ConsoleApp10
{
    internal class Program
    {
        static void Main(string[] args)
        {
            int plus = 12;
            int minus = 6;
            int gange = 3;
            int resultat = (plus - minus) * gange;
            Console.WriteLine("(12 - 6) * 3 = {0}", resultat);
            Console.ReadKey();
        }
    }
}
